{
  "manifest_version": 3,
  "name": "Website Color Changer By Rupam",
  "version": "1.0",
  "description": "Change the background color of any website with one click!",
  "permissions": ["scripting", "activeTab"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
